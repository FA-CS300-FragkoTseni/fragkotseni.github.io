// Fetch all movies
async function fetchAllMovies() {
  const response = await fetch("https://fa-cs300-fragkotseni.github.io/fragkotseni.github.io/Movies.json");
  const movies = await response.json();
  console.log("All Movies:", movies);
  return movies;
}

// Fetch all books
async function fetchAllBooks() {
  const response = await fetch("https://fa-cs300-fragkotseni.github.io/fragkotseni.github.io/Book.json");
  const books = await response.json();
  console.log("All Books:", books);
  return books;
}

// Run them
fetchAllMovies();
fetchAllBooks();